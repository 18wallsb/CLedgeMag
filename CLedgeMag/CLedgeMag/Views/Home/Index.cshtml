<body>
    <audio src="~/Content/Boat Harbor Ambience Sound Effect.mp3" autoplay loop="loop"></audio>

    <div class="intro">
        <h1 class="moving_text zinesHeader" id="textNoMove">43.0653° N, 88.1087° W</h1>
        <h1 class="moving_text zinesHeader" id="textMove">43.0653° N, 88.1087° W</h1>
    </div>


</body>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>
    $(document).mousemove(function(e){
        var offLeft;
        var offTop;
        var currtop = $("#textNoMove").css('top');
        var currleft =$("#textNoMove").css('left');
        var splitTop= currtop.split('p');
        var splitLeft = currleft.split('p');
        var intTop = parseInt( splitTop[0]);
        var intLeft =  parseInt(splitLeft[0]);
        $("#textMove").css({left:( intLeft- (.03 * (e.pageX-intLeft) ) ), top:(intTop - (.03 * (e.pageY-intTop) ))});


    });

</script>
